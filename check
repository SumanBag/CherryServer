    <script type="text/javascript">
        javascript: history.forward();
        $(document).ready(function() {
            $("#radio_submit").click(function (e) {
                var rollno = document.getElementById("rollno").value;
                var questions = $( ".divider" ).size();                
                for (var i = 1; i <= questions; i++)
                {
                    radio_name = 'rad'+i+'[]';

                    question_id = 'question_id'+i;

                    var question_id = document.getElementById(question_id).value;

                    var rates = document.getElementsByName(radio_name);
                    var rate_value = 0;
                    for(var j = 0; j < rates.length; j++){
                        if(rates[j].checked){
                            rate_value = rates[j].value;
                        }
                    }

                    if(parseInt(rate_value,10) != 0){
                        $.ajax({url: "../view/updateReply.php?value="+rate_value+"&id="+question_id+"&rollno="+rollno, success: function(result){
                            $("#div1").html(result);
                        }});
                    }
                }
            });

            $('.rg').change(function(){                                      
                var radioid = $(this).attr('id');               
                var radioname = $(this).attr('name');                      
                var id = radioid.substring(3);

                var button_id = 'button'+id;                
                var button = document.getElementById(button_id);
                button.style.background = "green";

                var rollno = $('#rollno').val();                         
                var question_id =  $("#" + "question_id" + id).val();                                     
                var radios = $('[name="' + radioname + '"]');               
                var radiovalue = 0;
                for(var j = 0; j < radios.length; j++){
                    if(radios[j].checked){
                        radiovalue = radios[j].value;
                    }
                } 

                if(parseInt(radiovalue,10) != 0){
                    $.ajax({url: "../view/updateReply.php?value="+radiovalue+"&id="+question_id+"&rollno="+rollno, success: function(result){
                        $("#div1").html(result);
                    }});
                }             

            });

            $('.rg').mousedown(function() { 

                if(this.checked) {

                    $(this).mouseup(function() {  

                        var radio = this;
                        var radioid = $(this).attr('id');               
                        var radioname = $(this).attr('name');                      
                        var id = radioid.substring(3);

                        var button_id = 'button'+id;                
                        var button = document.getElementById(button_id);
                        button.style.background = "red";

                        var rollno = $('#rollno').val();                         
                        var question_id =  $("#" + "question_id" + id).val();

                        $.ajax({url: "../view/updateReply.php?value=0"+"&id="+question_id+"&rollno="+rollno, success: function(result){
                            $("#div1").html(result);
                        }});
                        setTimeout(function() { 
                            radio.checked = false; 
                        }, 5); 

                        $(this).unbind('mouseup');
                    });
                }
            });
            $('.uncheck').click(function() {
                var button = this;
                var buttonid = $(this).attr('id');                
                var id = buttonid.substring(6);               
                self.location.href = '#link' + id;

            });
            $('.check').click(function() {
                var button = this;
                var buttonid = $(this).attr('id');                
                var id = buttonid.substring(6);               
                self.location.href = '#link' + id;

            });  

        });

function submitme()
{
    var check = confirm("Are you sure to Submit ?");
    if (check) {
        document.getElementById("form1").action = "../view/result.php";
        document.forms['form1'].submit();
    }
    return false;
}

var delay = 0;

var Timer;
var TotalSeconds;

function CreateTimer(TimerID, delay, dur) {
    Timer = document.getElementById(TimerID);
    var elapsedTime =  parseInt(delay);
    if (isNaN(elapsedTime))
        elapsedTime = 0;
    TotalSeconds = parseInt(dur) - elapsedTime;
    if (TotalSeconds < 0)
        TotalSeconds = 0;

    UpdateTimer()
    window.setTimeout("Tick()", 1000);
}

function Tick() {
    if (TotalSeconds == 0)
    {
        alert("Your Time's up!!!");
        document.getElementById("form1").action = "../view/result.php";
        document.forms['form1'].submit();
    }
    else
    {
        TotalSeconds -= 1;
        UpdateTimer()
        window.setTimeout("Tick()", 1000);
    }
}

function UpdateTimer() {
    var obj = secondsToTime(TotalSeconds);
    Timer.innerHTML = obj;
}

function secondsToTime(secs) {
    var hours = Math.floor(secs / (60 * 60));
    var divisor_for_minutes = secs % (60 * 60);
    var minutes = Math.floor(divisor_for_minutes / 60);
    var divisor_for_seconds = divisor_for_minutes % 60;
    var seconds = Math.ceil(divisor_for_seconds);

    return hours + ":" + minutes + ":" + seconds;
}

window.onscroll = function() {scrollFunction()};

function scrollFunction() {
    if (document.body.scrollTop > 20 || document.documentElement.scrollTop > 20) {
        document.getElementById("myBtn").style.display = "block";
    } else {
        document.getElementById("myBtn").style.display = "none";
    }
    document.getElementById("div1").innerHTML = "";
}

function topFunction() {
    document.body.scrollTop = 0;
    document.documentElement.scrollTop = 0;
}

document.oncontextmenu = function () {
    return false;
};
</script>
